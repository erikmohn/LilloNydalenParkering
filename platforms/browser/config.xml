<?xml version='1.0' encoding='utf-8'?>
<widget id="no.fernanda.nissen.parkering" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Lillo nydalen Parkering</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="mohn.erik@gmail.com" href="http://cordova.io">
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
